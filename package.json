{
  "name": "gsmart",
  "version": "0.11.1",
  "description": "CLI to generate smart commit messages using AI.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragnarok22/gsmart.git"
  },
  "homepage": "https://ragnarok22.github.io/gsmart",
  "keywords": [
    "cli",
    "ai",
    "git",
    "gsmart",
    "commit",
    "conventional-commits",
    "openai",
    "anthropic",
    "gemini",
    "mistral"
  ],
  "bugs": {
    "url": "https://github.com/ragnarok22/gsmart/issues"
  },
  "author": "Reinier Hern√°ndez <sasuke.reinier@gmail.com>",
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gsmart": "dist/index.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "prebuild": "node prebuild.js",
    "build": "tsup",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "test": "tsx --test test/**/*.test.ts",
    "test:coverage": "tsx --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=coverage/lcov.info --test-reporter=spec --test-reporter-destination=stdout test/**/*.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/mistral": "^3.0.9",
    "@ai-sdk/openai": "^3.0.12",
    "ai": "^6.0.40",
    "chalk": "5.6.2",
    "clipboardy": "^5.0.2",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "update-notifier": "^7.3.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@types/prompts": "^2.4.9",
    "@types/update-notifier": "^6.0.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "esmock": "^2.7.3",
    "globals": "^17.0.0",
    "prettier": "3.8.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316"
}
